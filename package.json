{
  "name": "manybunnies",
  "version": "1.0.0",
  "description": "Site with Bunnies",
  "author": "Ben Pevsner",
  "license": "MIT",
  "main": "server.js",
  "homepage": "https://github.com/ivebencrazy/ManyBunnies#readme",
  "bugs": {
    "url": "https://github.com/ivebencrazy/ManyBunnies/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ivebencrazy/ManyBunnies.git"
  },
  "keywords": [
    "bunnies"
  ],
  "scripts": {
    "build": "rollup -c ./rollup.config.js --f iife",
    "start": "npm-run-all -p watch server",
    "server": "nodemon ./server.js",
    "test": "jest",
    "watch": "rollup -c ./rollup.config.js --f iife --watch"
  },
  "dependencies": {
    "lodash-es": "^4.17.5",
    "nodemon": "^1.17.1",
    "pixi.js": "^4.7.0",
    "rxjs": "^5.5.6"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.40",
    "babel-jest": "^22.4.1",
    "babel-preset-env": "^1.6.1",
    "husky": "^0.15.0-rc.8",
    "jest": "^22.4.2",
    "npm-run-all": "^4.1.2",
    "rollup": "^0.56.3",
    "rollup-plugin-commonjs": "^8.3.0",
    "rollup-plugin-node-resolve": "^3.0.3"
  },
  "babel": {
    "presets": [
      "env"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run build && git add ./docs/build/client.js"
    }
  }
}
